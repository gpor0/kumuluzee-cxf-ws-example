         version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee">

    <servlet>
        <display-name>KumuluzEECxfServlet</display-name>
        <servlet-name>KumuluzEECxfServlet</servlet-name>
        <servlet-class>com.kumuluz.ee.jaxws.cxf.KumuluzCXFServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
